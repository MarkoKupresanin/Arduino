int LightOne = 2;
int LightTwo = 4;
int LightThree = 6;  
int LightFour = 8;

void setup() {
  // put your setup code here, to run once:
pinMode(LightOne,OUTPUT);
pinMode(LightTwo,OUTPUT);
pinMode(LightThree,OUTPUT);
pinMode(LightFour,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightTwo,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightTwo,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightOne,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightOne,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightOne,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightOne,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,LOW);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,LOW);
delay(1000);
digitalWrite(LightOne,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightThree,LOW);
delay(400);
digitalWrite(LightOne,HIGH);
digitalWrite(LightTwo,HIGH);
digitalWrite(LightThree,HIGH);
digitalWrite(LightFour,HIGH);
delay(1000);
digitalWrite(LightFour,LOW);
digitalWrite(LightThree,LOW);
digitalWrite(LightTwo,LOW);
digitalWrite(LightOne,LOW);
delay(900);
}
