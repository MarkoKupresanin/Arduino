int redPin = 3;
int MaxBright = 255;
int MidBright = 100;
int LowBright = 5;
int Wait = 5000;

void setup() {
  pinMode(redPin, OUTPUT);
}

void loop() {
  analogWrite(redPin, LowBright);
  delay(Wait);
  analogWrite(redPin,MidBright);
  delay(Wait);
  analogWrite(redPin,MaxBright);
  delay(5000);
}
